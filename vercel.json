{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Frame-Options",
            "value": "ALLOW-FROM https://jeremiah.dev"
          },
          {
            "key": "Content-Security-Policy",
            "value": "frame-ancestors 'self' https://jeremiah.dev;"
          }
        ]
      }
    ]
}
  